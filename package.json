{
  "name": "fffunction",
  "version": "0.0.2",
  "description": "Create polymorphic functions in typescript.",
  "packageManager": "pnpm@9.15.0",
  "main": "dist/index.js",
  "sideEffects": false,
  "devDependencies": {
    "@types/jest": "29.5.12",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-expect": "1.3.0",
    "ts-jest": "29.1.2",
    "ts-pattern": "^5.0.6",
    "typescript": "5.3.3"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "test": "jest --coverage && npm run compile",
    "compile": "tsc -p ./tsconfig.test.json --noEmit",
    "prettier": "prettier '(src|tests)/**/*.ts' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrgautier/fffunction.git"
  },
  "keywords": [
    "utils",
    "typescript",
    "polymorph",
    "polymorphism",
    "function"
  ],
  "author": "Adrien Gautier",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/tests/.*.test.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
