{
  "name": "fffunction",
  "version": "0.0.2",
  "description": "Create polymorphic functions in typescript.",
  "packageManager": "pnpm@9.15.0",
  "main": "dist/index.js",
  "sideEffects": false,
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@types/jest": "29.5.12",
    "jest": "29.4.0",
    "rimraf": "6.0.1",
    "ts-expect": "1.3.0",
    "ts-jest": "29.4.0",
    "ts-pattern": "5.8.0",
    "typescript": "5.9.2"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "test": "jest --coverage && npm run compile",
    "compile": "tsc -p ./tsconfig.test.json --noEmit",
    "format": "biome format --write src tests",
    "lint": "biome lint src tests",
    "lint:fix": "biome lint --write src tests",
    "check": "biome check src tests",
    "check:fix": "biome check --write src tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adrgautier/fffunction.git"
  },
  "keywords": [
    "utils",
    "typescript",
    "polymorph",
    "polymorphism",
    "function"
  ],
  "author": "Adrien Gautier",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/tests/.*.test.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
